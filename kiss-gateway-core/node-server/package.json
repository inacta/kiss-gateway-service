    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/node": "^12.12.14",
    "@types/request-promise": "^4.1.45",
    "@types/supertest": "^2.0.8",
    "@types/swagger-ui-express": "^4.1.0",
    "basic-auth": "^2.0.1",
    "bignumber.js": "^9.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
